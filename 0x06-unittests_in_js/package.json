    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "sinon": "^15.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thala254/alx-backend-javascript"
  },
  "engine": {
    "node": "16.x",
    "npm": "8.x",
    "yarn": "1.x"
  }
}
